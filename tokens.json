{
  "Gmail": {
    "Semantics": {
      "border-color": {
        "$type": "color",
        "$value": "#ff006aff"
      },
      "border-1": {
        "$type": "border",
        "$value": {
          "color": "{border-color}",
          "width": "2px",
          "style": "solid"
        }
      }
    },
    "Core": {
      "white": {
        "$type": "color",
        "$value": "#ffffffff"
      }
    }
  },
  "Gmail[Default]": {
    "Semantics": {
      "border-1": {
        "$type": "border",
        "$value": {
          "color": "#ffee00ff",
          "width": "0px",
          "style": {
            "lineCap": "butt",
            "dashArray": [
              "21px",
              "42px"
            ]
          }
        }
      }
    }
  }
}